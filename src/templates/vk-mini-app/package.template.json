  "license": "MIT",
  "scripts": {
    "rm-source-maps": "rimraf public/**/*.js.map",
    "dev": "NODE_ENV=development API_URL=/api/ webpack serve --mode development",
    "build": "NODE_ENV=production webpack --mode production && yarn run rm-source-maps",
    "build-zip": "NODE_ENV=production IS_ZIP=true webpack --mode production",
    "ts-check": "tsc --skipLibCheck --noEmit --project tsconfig.json",
    "lint-fix": "eslint src --fix",
    "codestyle-fix": "prettier --write **/*.{ts,tsx,json,scss,css}",
    "deploy-vk": "CI_URLS=true vk-miniapps-deploy",
    "precommit": "lint-staged"
  },
  "homepage": "./",
  "commands": [
    "yarn build"
  ],
  "lint-staged": {
    "*.{ts,js}?(x)": [
      "yarn run lint-fix",
      "yarn run codestyle-fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run ts-check && yarn run lint-fix && yarn run codestyle-fix"
    }
  },
